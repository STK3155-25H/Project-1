{
    "git.autofetch": true,
    "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
    "files.autoSave": "afterDelay",
"latex-workshop.latex.tools": [
    {
        "name": "pdflatex",
        "command": "/Library/TeX/texbin/pdflatex",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "%DOC%"
        ]
    }
],
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex",
            "tools": ["pdflatex"]
        }
    ]
}
